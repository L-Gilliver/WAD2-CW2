<html>
    <head>
        <meta charset="UTF-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <title>West Coast Restaurant</title>
    </head>
    <body>
        <nav>
            <a href="/">
                About Us
            </a>
            <a href="/lunch-menu">
                Lunch Menu
            </a>
            <a href="/dinner-menu">
                Dinner Menu
            </a>
            <a href="/regional-menu">
                Regional Menu
            </a>
            <a href="#contact-anchor">
                Get In Touch
            </a>
        </nav>
        <h1>{{title}}</h1>
        <div id="specials">
            <h3>CHEF'S SPECIALS</h3>
            <div>
                {{#specialsStarter}}
                    <div>
                        <h6>{{dishName}}</h6>
                        <p>{{description}}</p>
                        <p>Contains: {{ingredients}}</p>
                        <p>Contains {{allergens}}</p>
                        <p>{{price}}</p>
                    </div>
                {{/specialsStarter}}
            </div>
            <div>
                {{#specialsMain}}
                    <div>
                        <h6>{{dishName}}</h6>
                        <p>{{description}}</p>
                        <p>Contains: {{ingredients}}</p>
                        <p>Contains {{allergens}}</p>
                        <p>{{price}}</p>
                    </div>
                {{/specialsMain}}
            </div>
        </div>
        <div>
            <h3>STARTERS</h3>
            {{#starters}}
                <div>
                    <h6>{{dishName}}</h6>
                    <p>{{description}}</p>
                    <p>Contains: {{ingredients}}</p>
                    <p>Contains {{allergens}}</p>
                    <p>{{price}}</p>
                </div>
            {{/starters}}
        </div>
        <div>
            <h3>MAINS</h3>
            {{#mains}}
                <div>
                    <h6>{{dishName}}</h6>
                    <p>{{description}}</p>
                    <p>Contains: {{ingredients}}</p>
                    <p>Contains {{allergens}}</p>
                    <p>{{price}}</p>
                </div>
            {{/mains}}
        </div>
        <div>
            <h3>DESSERTS</h3>
            {{#desserts}}
                <div>
                    <h6>{{dishName}}</h6>
                    <p>{{description}}</p>
                    <p>Contains: {{ingredients}}</p>
                    <p>Contains {{allergens}}</p>
                    <p>{{price}}</p>
                </div>
            {{/desserts}}
        </div>
    </body>
    <footer>
        <p>contact info</p>
    </footer>
</html>